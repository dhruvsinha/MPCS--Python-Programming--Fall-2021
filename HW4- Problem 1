def prime_generator():
    number=3
    while True:
        is_prime=1
        for i in range(2,number):
            if(number%i)==0:
                is_prime=0
                break
        if is_prime==1:
            yield(number)
        
        number=number+1
        
prime_list= prime_generator()

for prime in range(10):
    print (next(prime_list))
